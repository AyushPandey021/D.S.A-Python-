    "# check number pilemdrone or not \n",
    "n= 8558\n",
    "num = n \n",
    "res = 0\n",
    "while num>0:\n",
    "  id = num%10\n",
    "  res = (res*10)+id\n",
    "  num//=10\n",
    "print( \"Reverse :\",res+num) #reverse your number\n",
    "if n==res:\n",
    "  print(\"Palindrome Yes\")\n",
    "else:\n",
    "  print(\"No Palindrome\")"
   ]
