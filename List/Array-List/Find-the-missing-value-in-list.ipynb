    "# find the missing value\n",
    "ap= [335,337,338,339,340]\n",
    "\n",
    "S = min(ap)\n",
    "E = max(ap)\n",
    "mix = E-S+1\n",
    "formula= (S+E)*mix//2\n",
    "plus = sum(ap)\n",
    "find_miss = formula-plus\n",
    "print(find_miss)\n"
   ]
