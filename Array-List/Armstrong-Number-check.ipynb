    "# Armstrong Number\n",
    "x = 153\n",
    "num= x\n",
    "check = 0\n",
    "n= len(str(x))\n",
    "while num >0:\n",
    "  id = num % 10 \n",
    "  check = check+(id**n)\n",
    "  num//=10 \n",
    "if x == check:\n",
    "  print(\"yes\")\n",
    "else:\n",
    "  print(\"not\")"
   ]
