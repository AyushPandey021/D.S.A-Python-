    "# fibonachi in recursion\n",
    "# imp \n",
    "def fib(n):\n",
    " if n == 0 :\n",
    "  return 0 \n",
    " if n == 1: \n",
    "  return 1\n",
    " return fib(n-1)+fib(n-2)\n",
    "def func(n):\n",
    " return fib(n)\n",
    "no=6\n",
    "print(func(no))\n",
    " "
